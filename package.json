{
  "name": "glukograd",
  "version": "1.0.0",
  "description": "Glukograd",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run dev:server",
    "dev:client": "webpack serve --mode development",
    "dev:server": "nodemon",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production",
    "build:server": "tsc --project tsconfig.server.json",
    "start": "node dist/index.js",
    "clean": "rm -rf dist .tsbuildinfo client/public/bundle*",
    "docker:build": "docker build -t glukograd-app:latest .",
    "swarm:deploy": "docker stack deploy -c docker-compose.yml bugacity",
    "swarm:rm": "docker stack rm bugacity",
    "swarm:logs": "docker service logs -f bugacity_app | cat"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@babylonjs/core": "^6.35.1",
    "@babylonjs/loaders": "^6.35.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
