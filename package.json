{
  "name": "glukograd",
  "version": "1.0.0",
  "scripts": {
    "dev": "dotenv -e .env.dev nodemon server/src/index.ts",
    "start": "dotenv -e .env ts-node server/src/index.ts",
    "build": "npm run build:client",
    "build:client": "webpack --mode production",
    "dev:client": "webpack --mode development --watch",
